{
  "name": "@the-oleg/the-utils",
  "version": "0.0.1",
  "description": "Simple reusable TypeScript utils for any JS project",
  "type": "commonjs",
  "main": "./dist/the-utils.cjs",
  "module": "./dist/the-utils.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/the-utils.es.js",
      "require": "./dist/the-utils.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Verbalman/the-utils.git"
  },
  "keywords": ["utility", "helping functions", "util", "utils", "helpers"],
  "author": "Oleg Kasianets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Verbalman/the-utils/issues"
  },
  "homepage": "https://github.com/Verbalman/the-utils#readme"
}
